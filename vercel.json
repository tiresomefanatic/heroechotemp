{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@vercel/node"
    }
  ],
  "framework": "nuxt",
  "buildCommand": "nuxt build",
  "devCommand": "nuxt dev",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/_nuxt/.*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/.*",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/.*",
      "dest": "/"
    }
  ]
}
