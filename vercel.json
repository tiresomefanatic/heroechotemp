{
  "version": 2,
  "buildCommand": "NITRO_PRESET=vercel npm run build",
  "builds": [
    {
      "src": ".vercel/output/functions/__nitro.func/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/__nitro"
    },
    {
      "src": "/_nuxt/(.*)",
      "dest": "/__nitro"
    },
    {
      "src": "/(.*)",
      "dest": "/__nitro"
    }
  ]
}
